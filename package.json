{
  "name": "boreal.chat",
  "private": true,
  "version": "0.1.4",
  "type": "module",
  "scripts": {
    "start": "node build",
    "dev": "vite dev --host",
    "dev:caddy": "caddy run --config infra/development/Caddyfile",
    "dev:stripe": "stripe listen --forward-to localhost:5173/api/webhook/stripe",
    "prebuild": "pnpm db:client:compile && pnpm script:fetchModels",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "pretypecheck": "pnpm i18n",
    "typecheck": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "typecheck:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "preformat": "pnpm i18n",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint": "pnpm i18n",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:client:generate": "drizzle-kit generate --schema ./src/lib/client/db/schema.ts --dialect postgresql --driver pglite --casing snake_case --out ./src/lib/client/db/migrations",
    "db:client:compile": "node scripts/compileMigrations.mjs",
    "db:client:build": "pnpm db:client:generate && pnpm db:client:compile",
    "db:build": "pnpm db:generate && pnpm db:client:build && pnpm db:migrate",
    "add-ui": "pnpm dlx shadcn-svelte@latest add",
    "build:docker": "docker build -t boreal.chat .",
    "i18n": "paraglide-js compile --outdir src/lib/paraglide",
    "script:fetchModels": "pnpm dlx bun run ./scripts/fetchModelFeatures.ts",
    "test": "vitest",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@chromatic-com/storybook": "^4.0.1",
    "@cloudflare/workers-types": "^4.20250607.0",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@hono/node-server": "^1.14.4",
    "@internationalized/date": "^3.8.1",
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.3.1",
    "@langchain/mcp-adapters": "^0.5.2",
    "@langchain/openai": "^0.5.12",
    "@lucide/svelte": "^0.515.0",
    "@orpc/server": "^1.6.8",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-svelte-csf": "^5.0.7",
    "@storybook/addon-vitest": "^9.0.17",
    "@storybook/sveltekit": "^9.0.17",
    "@sveltejs/adapter-cloudflare": "^7.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.0",
    "@testcontainers/postgresql": "^11.2.1",
    "@testcontainers/redis": "^11.2.1",
    "@types/async-lock": "^1.4.2",
    "@types/bun": "^1.2.17",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "bits-ui": "^2.8.10",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-storybook": "^9.0.17",
    "eslint-plugin-svelte": "^3.0.0",
    "exponential-backoff": "^3.1.2",
    "formsnap": "^2.0.1",
    "globals": "^16.0.0",
    "hono": "^4.7.11",
    "mdsvex": "^0.12.3",
    "mode-watcher": "^1.0.8",
    "openai": "^5.3.0",
    "p-queue": "^8.1.0",
    "paneforge": "1.0.0-next.5",
    "posthog-node": "^5.1.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^9.0.17",
    "stripe": "^18.3.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-seo": "^1.6.1",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.0",
    "testcontainers": "^11.2.1",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^6.2.6",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^3.2.4",
    "zod": "^3.25.64",
    "@vitest/browser": "3.2.4",
    "playwright": "^1.54.1",
    "@vitest/coverage-v8": "3.2.4"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.3",
    "@electric-sql/pglite-sync": "^0.3.6",
    "@humanspeak/svelte-markdown": "^0.8.7",
    "@humanspeak/svelte-virtual-list": "^0.2.5",
    "@inlang/paraglide-js": "^2.0.0",
    "@orpc/client": "^1.6.8",
    "@orpc/tanstack-query": "^1.6.8",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@remotion/media-parser": "4.0.322",
    "@sentry/sveltekit": "^9",
    "@tanstack/query-async-storage-persister": "^5.81.5",
    "@tanstack/svelte-query": "^5.81.5",
    "@tanstack/svelte-query-devtools": "^5.81.5",
    "@tanstack/svelte-query-persist-client": "^5.81.5",
    "arctic": "^3.7.0",
    "async-lock": "^1.4.1",
    "drizzle-orm": "^0.44.2",
    "fast-tree-builder": "^2.0.1",
    "flagsmith": "^9.3.0",
    "ioredis": "^5.6.1",
    "is-promise": "^4.0.0",
    "js-cookie": "^3.0.5",
    "katex": "^0.16.22",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "posthog-js": "^1.255.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "runed": "^0.30.0",
    "shiki": "^3.6.0",
    "svelte-exmarkdown": "^5.0.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "files": [
    "build"
  ],
  "packageManager": "pnpm@9.12.2"
}
